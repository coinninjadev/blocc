{
    "settings": {
        "index": {
            "number_of_replicas": 0,
            "number_of_shards": 25,
            "refresh_interval": "30s",
            "mapping.ignore_malformed": true
        }
    },
    "mappings": {
        "blocc": {
            "dynamic_templates": [
                {
                    "data": {
                        "match": "data.*",
                        "mapping": {
                            "norms": false,
                            "doc_values": false,
                            "fielddata": false,
                            "type": "keyword"
                        }
                    }
                },
                {
                    "star_as_keyword": {
                        "match_mapping_type": "*",
                        "mapping": {
                            "type": "keyword",
                            "norms": false
                        }
                    }
                }
            ],
            "properties": {
                "block_height": {
                    "type": "long"
                },
                "height": {
                    "type": "long"
                },
                "time": {
                    "type": "long"
                },
                "block_time": {
                    "type": "long"
                },
                "out": {
                    "type": "object",
                    "properties": {
                        "value": {
                            "type": "long"
                        }
                    }
                },
                "raw": {
                    "type": "binary"
                },
                "data": {
                    "type": "object"
                }
            }
        }
    }
}