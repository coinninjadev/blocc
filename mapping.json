{
    "settings": {
        "index": {
            "number_of_replicas": 0,
            "number_of_shards": 5,
            "refresh_interval": "30s",
            "mapping.ignore_malformed": true
        }
    },
    "mappings": {
        "blocc": {
            "dynamic_templates": [
                {
                    "object_as_object": {
                        "match_mapping_type": "object",
                        "mapping": {
                            "type": "object"
                        }
                    }
                },
                {
                    "metric": {
                        "match": "metric.*",
                        "mapping": {
                            "norms": false,
                            "type": "double"
                        }
                    }
                },
                {
                    "timestamp": {
                        "match": "*timestamp",
                        "mapping": {
                            "norms": false,
                            "type": "date"
                        }
                    }
                },
                {
                    "star_as_keyword": {
                        "match_mapping_type": "*",
                        "mapping": {
                            "type": "keyword",
                            "norms": false
                        }
                    }
                }
            ],
            "properties": {
                "type": {
                    "type": "keyword"
                },
                "blockId": {
                    "type": "keyword"
                },
                "height": {
                    "type": "long"
                },
                "prevBlockId": {
                    "type": "keyword"
                },
                "time": {
                    "type": "long"
                },
                "blockTime": {
                    "type": "long"
                },
                "address": {
                    "type": "keyword"
                },
                "txId": {
                    "type": "keyword"
                },
                "raw": {
                    "type": "binary"
                }
            }
        }
    }
}