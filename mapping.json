PUT /btc
{
    "settings": {
        "index": {
            "number_of_replicas": 0,
            "number_of_shards": 5,
            "refresh_interval": "30s",
            "mapping.ignore_malformed": true
        }
    },
    "mappings": {
        "btc": {
            "dynamic_templates": [
                {
                    "object_as_object": {
                        "match_mapping_type": "object",
                        "mapping": {
                            "type": "object"
                        }
                    }
                },
                {
                    "timestamp": {
                        "match": "*timestamp",
                        "mapping": {
                            "norms": false,
                            "type": "date"
                        }
                    }
                },
                {
                    "star_as_keyword": {
                        "match_mapping_type": "*",
                        "mapping": {
                            "type": "keyword",
                            "norms": false
                        }
                    }
                }
            ]
        }
    }
}